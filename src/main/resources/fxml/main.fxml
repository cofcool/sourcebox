<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
    <MenuBar fx:id="menu">
        <Menu fx:id="save" text="保存"/>
        <Menu text="Run"/>
    </MenuBar>

    <TabPane tabClosingPolicy="UNAVAILABLE">
        <Tab text="Converts">
            <fx:include source="converts.fxml"/>
        </Tab>
        <Tab text="Action Note">
            <fx:include source="note-action.fxml"/>
        </Tab>
        <Tab text="Note">
            <fx:include source="note.fxml"/>
        </Tab>
    </TabPane>
</VBox>
